{
  "entrypoint": "index.html",
  "shell": "dist/elements/app.js",
  "sources": [
   "dist/**/*.js",
   "package.json",
   "manifest.json"
  ],
  "extraDependencies": [
    "env.js",
    "sw.js",
    "manifest.json",
    "node_modules/@webcomponents/webcomponentsjs/**/*",
    "node_modules/jsqr/dist/jsQR.js",
    "node_modules/workbox-sw/build/workbox-sw.js",
    "assets/**/*",
    "vendor/**/*",
    "shim/**/*"
  ],
  "moduleResolution": "node",
  "npm": true,
  "lint": {
    "ignoreWarnings": [
      "could-not-resolve-reference",
      "cant-determine-element-tagname"
    ]
  },
  "builds": [
    {
      "name": "pwa",
      "browserCapabilities": [
        "es2017"
      ],
      "js": {
        "minify": false,
        "compile": false,
        "transformModulesToAmd": false
      },
      "css": {
        "minify": false
      },
      "html": {
        "minify": false
      },
      "bundle": false
    }
  ]
}
